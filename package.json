{
  "name": "babel-plugin-react-css-module",
  "version": "1.0.0-beta.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --colors",
    "check": "npm run prettier -- -l",
    "format": "npm run prettier -- --write",
    "prettier": "prettier \"src/**/*.ts\"",
    "build": "rm -rf lib && tsc -d",
    "ci": "npm run check && npm run test"
  },
  "author": "duxiaodong",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/helper-plugin-test-runner": "^7.1.0",
    "@babel/helper-plugin-utils": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@types/babel__core": "^7.0.4",
    "@types/babel__generator": "^7.0.1",
    "@types/babel__template": "^7.0.1",
    "@types/babel__traverse": "^7.0.4",
    "@types/node": "^10.12.12",
    "babel-core": "^7.0.0-bridge.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.1"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "/test/index.js$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run ci"
    }
  }
}
